{
  // Uncomment and change port here if needed
  // "Kestrel:Endpoints:Http:Url": "http://localhost:5000",

  "BackendOptions": {
    "TelegramBotTokenHash": ""
  },

  // Don't change unless you absolutely sure
  "TonOptions": {
    "UseMainnet": true,
    "VerbosityLevel": 0,
    "LogTextLimit": 1500
  },

  // Logging settings
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "TonLibDotNet.TonClient": "Information",
      "RecurrentTasks": "Warning",
      "Microsoft": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  },

  // Do not change unless you already know you should
  "AllowedHosts": "*"
}
